@model IEnumerable<BugTracker.Models.ApplicationUser>
@{
    ViewBag.Title = "SelectUser";
}


<h2>SelectUser</h2>
    <ul>
        @foreach (var user in Model)
        {
            <li><a href="@Url.Action("EditUser", "Home", new { id = @user.Id })"> @Html.DisplayFor(model => user.DisplayName)</a></li>
        }
    </ul>


